          name="Hjid"
          type="org.hibernate.type.LongType">
         <generator class="native"/>
      </id>
      <discriminator type="org.hibernate.type.StringType">
         <column name="Hjtype"/>
      </discriminator>
      <property access="org.jvnet.hyperjaxb2.runtime.hibernate.accessor.CheckingPropertyAccessor"
                name="Description">
         <column name="Description"/>
         <type name="org.hibernate.type.StringType"/>
      </property>
      <many-to-one class="org.w3._2001.xmlschema.AnyType" name="Type">
         <column name="Type_"/>
      </many-to-one>
      <property access="org.jvnet.hyperjaxb2.runtime.hibernate.accessor.CheckingPropertyAccessor"
                name="Property">
         <column name="Property"/>
         <type name="org.hibernate.type.StringType"/>
      </property>
      <subclass discriminator-value="org.morphml.metadata.schema.PropertyDetail"
                name="org.morphml.metadata.schema.impl.PropertyDetailImpl"/>
   </class>
</hibernate-mapping>