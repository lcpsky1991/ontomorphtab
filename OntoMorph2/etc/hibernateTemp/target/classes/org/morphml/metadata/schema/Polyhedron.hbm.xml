<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping
  PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping auto-import="false" default-cascade="all-delete-orphan" default-lazy="false">
   <class discriminator-value="[org.morphml.metadata.schema.Polyhedron]"
          name="org.morphml.metadata.schema.Polyhedron"
          table="Polyhedron">
      <id access="org.jvnet.hyperjaxb2.runtime.hibernate.accessor.CheckingPropertyAccessor"
          name="Hjid"
          type="org.hibernate.type.LongType">
         <generator class="native"/>
      </id>
      <discriminator type="org.hibernate.type.StringType">
         <column name="Hjtype"/>
      </discriminator>
      <many-to-one class="org.morphml.metadata.schema.Polyhedron$PolygonsType" name="Polygons">
         <column name="Polygons"/>
      </many-to-one>
      <subclass discriminator-value="org.morphml.metadata.schema.Polyhedron"
                name="org.morphml.metadata.schema.impl.PolyhedronImpl"/>
   </class>
</hibernate-mapping>