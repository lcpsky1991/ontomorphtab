                name="Metric">
         <column name="Metric"/>
         <type name="org.hibernate.type.StringType"/>
      </property>
      <many-to-one class="org.morphml.morphml.schema.InhomogeneousParam$ProximalType"
                   name="Proximal">
         <column name="Proximal"/>
      </many-to-one>
      <many-to-one class="org.morphml.morphml.schema.InhomogeneousParam$DistalType" name="Distal">
         <column name="Distal"/>
      </many-to-one>
      <subclass discriminator-value="org.morphml.morphml.schema.InhomogeneousParam"
