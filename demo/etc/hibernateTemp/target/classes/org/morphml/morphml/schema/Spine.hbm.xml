<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping
  PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping auto-import="false" default-cascade="all-delete-orphan" default-lazy="false">
   <class discriminator-value="[org.morphml.morphml.schema.Spine]"
          name="org.morphml.morphml.schema.Spine"
          table="Spine">
      <id access="org.jvnet.hyperjaxb2.runtime.hibernate.accessor.CheckingPropertyAccessor"
          name="Hjid"
          type="org.hibernate.type.LongType">
         <generator class="native"/>
      </id>
      <discriminator type="org.hibernate.type.StringType">
         <column name="Hjtype"/>
      </discriminator>
      <property access="org.jvnet.hyperjaxb2.runtime.hibernate.accessor.CheckingPropertyAccessor"
                name="Shape">
         <column name="Shape"/>
         <type name="org.hibernate.type.StringType"/>
      </property>
      <property access="org.jvnet.hyperjaxb2.runtime.hibernate.accessor.CheckingPropertyAccessor"
                name="Parent">
         <column name="Parent"/>
         <type name="org.hibernate.type.BigIntegerType"/>
      </property>
      <property access="org.jvnet.hyperjaxb2.runtime.hibernate.accessor.CheckingPropertyAccessor"
                name="Volume">
         <column name="Volume"/>
         <type name="org.hibernate.type.DoubleType"/>
      </property>
      <property access="org.jvnet.hyperjaxb2.runtime.hibernate.accessor.CheckingPropertyAccessor"
                name="Length">
         <column name="Length_"/>
         <type name="org.hibernate.type.DoubleType"/>
      </property>
      <many-to-one class="org.morphml.metadata.schema.Point" name="Proximal">
         <column name="Proximal"/>
      </many-to-one>
      <many-to-one class="org.morphml.metadata.schema.Point" name="Distal">
         <column name="Distal"/>
      </many-to-one>
      <subclass discriminator-value="org.morphml.morphml.schema.Spine"
                name="org.morphml.morphml.schema.impl.SpineImpl"/>
   </class>
</hibernate-mapping>