<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping
  PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping auto-import="false" default-cascade="all-delete-orphan" default-lazy="false">
   <class discriminator-value="[org.morphml.morphml.schema.Cell]" lazy="true"
          name="org.morphml.morphml.schema.Cell"
          proxy="org.morphml.morphml.schema.Cell"
          table="Cell">
      <id access="org.jvnet.hyperjaxb2.runtime.hibernate.accessor.CheckingPropertyAccessor"
          name="Hjid"
          type="org.hibernate.type.LongType">
         <generator class="native"/>
      </id>
      <discriminator type="org.hibernate.type.StringType">
         <column name="Hjtype"/>
      </discriminator>
      <many-to-one class="org.morphml.morphml.schema.Cell$CellBodyType" name="CellBody">
         <column name="CellBody"/>
      </many-to-one>
      <list access="org.jvnet.hyperjaxb2.runtime.hibernate.accessor.ListAccessor"
            name="Segments"
            table="Cell_Segments">
         <key>
            <column name="Cell_Segments_Hjid"/>
         </key>
         <list-index>
            <column name="Cell_Segments_Hjindex"/>
         </list-index>
         <one-to-many class="org.morphml.morphml.schema.Cell$SegmentsType"/>
      </list>
      <many-to-one class="org.morphml.metadata.schema.Authors" name="AuthorList">
         <column name="AuthorList"/>
      </many-to-one>
      <many-to-one class="org.morphml.morphml.schema.Cell$SpinesType" name="Spines">
         <column name="Spines"/>
      </many-to-one>
      <list access="org.jvnet.hyperjaxb2.runtime.hibernate.accessor.ListAccessor"
            name="Group"
            table="Cell_Group_">
         <key>
            <column name="Cell_Group__Hjid"/>
         </key>
         <list-index>
            <column name="Cell_Group__Hjindex"/>
         </list-index>
         <element type="org.hibernate.type.StringType">
            <column name="Hjvalue"/>
         </element>
      </list>
      <many-to-one class="org.morphml.metadata.schema.Properties" name="Properties">
         <column name="Properties"/>
      </many-to-one>
      <property access="org.jvnet.hyperjaxb2.runtime.hibernate.accessor.CheckingPropertyAccessor"
                name="Name">
         <column name="Name_"/>
         <type name="org.hibernate.type.StringType"/>
      </property>
      <property access="org.jvnet.hyperjaxb2.runtime.hibernate.accessor.CheckingPropertyAccessor"
                name="Notes">
         <column name="Notes"/>
         <type name="org.hibernate.type.StringType"/>
      </property>
      <many-to-one class="org.morphml.metadata.schema.ModelDBReference" name="ModelDBref">
         <column name="ModelDBref"/>
      </many-to-one>
      <many-to-one class="org.morphml.metadata.schema.Annotation" name="Annotation">
         <column name="Annotation"/>
      </many-to-one>
      <list access="org.jvnet.hyperjaxb2.runtime.hibernate.accessor.ListAccessor"
            name="Publication"
            table="Cell_Publication">
         <key>
            <column name="Cell_Publication_Hjid"/>
         </key>
         <list-index>
            <column name="Cell_Publication_Hjindex"/>
         </list-index>
         <one-to-many class="org.morphml.metadata.schema.Publication"/>
      </list>
      <many-to-one class="org.morphml.morphml.schema.FreePoints" name="FreePoints">
         <column name="FreePoints"/>
      </many-to-one>
      <many-to-one class="org.morphml.metadata.schema.NeuronDBReference" name="NeuronDBref">
         <column name="NeuronDBref"/>
      </many-to-one>
      <many-to-one class="org.morphml.morphml.schema.Cell$CablesType" name="Cables">
         <column name="Cables"/>
      </many-to-one>
      <subclass discriminator-value="org.morphml.morphml.schema.Cell" lazy="true"
                name="org.morphml.morphml.schema.impl.CellImpl"
                proxy="org.morphml.morphml.schema.Cell"/>
   </class>
</hibernate-mapping>