<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping
  PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping auto-import="false" default-cascade="all-delete-orphan" default-lazy="false">
   <subclass discriminator-value="[org.morphml.channelml.schema.BlockingSynapse]"
             extends="org.morphml.channelml.schema.DoubleExponentialSynapse"
             name="org.morphml.channelml.schema.BlockingSynapse">
      <join table="BlockingSynapse">
         <key>
            <column name="Hjid"/>
         </key>
         <many-to-one class="org.morphml.channelml.schema.Block" name="Block">
            <column name="Block"/>
         </many-to-one>
      </join>
      <subclass discriminator-value="org.morphml.channelml.schema.BlockingSynapse"
                name="org.morphml.channelml.schema.impl.BlockingSynapseImpl"/>
   </subclass>
</hibernate-mapping>