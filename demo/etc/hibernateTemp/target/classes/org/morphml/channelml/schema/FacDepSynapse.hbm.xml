<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping
  PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping auto-import="false" default-cascade="all-delete-orphan" default-lazy="false">
   <subclass discriminator-value="[org.morphml.channelml.schema.FacDepSynapse]"
             extends="org.morphml.channelml.schema.MultiDecaySynapse"
             name="org.morphml.channelml.schema.FacDepSynapse">
      <join table="FacDepSynapse">
         <key>
            <column name="Hjid"/>
         </key>
         <many-to-one class="org.morphml.channelml.schema.FacDep" name="Plasticity">
            <column name="Plasticity"/>
         </many-to-one>
      </join>
      <subclass discriminator-value="org.morphml.channelml.schema.FacDepSynapse"
                name="org.morphml.channelml.schema.impl.FacDepSynapseImpl"/>
   </subclass>
</hibernate-mapping>