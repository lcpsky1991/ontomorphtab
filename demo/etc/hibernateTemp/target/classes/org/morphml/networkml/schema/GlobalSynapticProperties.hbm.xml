<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping
  PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping auto-import="false" default-cascade="all-delete-orphan" default-lazy="false">
   <subclass discriminator-value="[org.morphml.networkml.schema.GlobalSynapticProperties]"
             extends="org.morphml.networkml.schema.SynapseInternalProperties"
             name="org.morphml.networkml.schema.GlobalSynapticProperties">
      <join table="GlobalSynapticProperties">
         <key>
            <column name="Hjid"/>
         </key>
         <property access="org.jvnet.hyperjaxb2.runtime.hibernate.accessor.CheckingPropertyAccessor"
                   name="SynapseType">
            <column name="SynapseType"/>
            <type name="org.hibernate.type.StringType"/>
         </property>
         <many-to-one class="org.morphml.networkml.schema.SynapseInternalProperties"
                      name="DefaultValues">
            <column name="DefaultValues"/>
         </many-to-one>
         <property access="org.jvnet.hyperjaxb2.runtime.hibernate.accessor.CheckingPropertyAccessor"
                   name="SynapseTypeValue">
            <column name="SynapseTypeValue"/>
            <type name="org.hibernate.type.StringType"/>
         </property>
      </join>
      <subclass discriminator-value="org.morphml.networkml.schema.GlobalSynapticProperties"
                name="org.morphml.networkml.schema.impl.GlobalSynapticPropertiesImpl"/>
   </subclass>
</hibernate-mapping>