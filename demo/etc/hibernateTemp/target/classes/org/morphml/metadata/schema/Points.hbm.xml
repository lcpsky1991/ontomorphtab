<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping
  PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping auto-import="false" default-cascade="all-delete-orphan" default-lazy="false">
   <class discriminator-value="[org.morphml.metadata.schema.Points]"
          name="org.morphml.metadata.schema.Points"
          table="Points">
      <id access="org.jvnet.hyperjaxb2.runtime.hibernate.accessor.CheckingPropertyAccessor"
          name="Hjid"
          type="org.hibernate.type.LongType">
         <generator class="native"/>
      </id>
      <discriminator type="org.hibernate.type.StringType">
         <column name="Hjtype"/>
      </discriminator>
      <property access="org.jvnet.hyperjaxb2.runtime.hibernate.accessor.CheckingPropertyAccessor"
                name="Name">
         <column name="Name_"/>
         <type name="org.hibernate.type.StringType"/>
      </property>
      <list access="org.jvnet.hyperjaxb2.runtime.hibernate.accessor.ListAccessor"
            name="Point"
            table="Points_Point">
         <key>
            <column name="Points_Point_Hjid"/>
         </key>
         <list-index>
            <column name="Points_Point_Hjindex"/>
         </list-index>
         <one-to-many class="org.morphml.metadata.schema.Point"/>
      </list>
      <subclass discriminator-value="org.morphml.metadata.schema.Points"
                name="org.morphml.metadata.schema.impl.PointsImpl"/>
   </class>
</hibernate-mapping>