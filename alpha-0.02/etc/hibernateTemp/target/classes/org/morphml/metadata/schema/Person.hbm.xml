<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping
  PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping auto-import="false" default-cascade="all-delete-orphan" default-lazy="false">
   <class discriminator-value="[org.morphml.metadata.schema.Person]"
          name="org.morphml.metadata.schema.Person"
          table="Person">
      <id access="org.jvnet.hyperjaxb2.runtime.hibernate.accessor.CheckingPropertyAccessor"
          name="Hjid"
          type="org.hibernate.type.LongType">
         <generator class="native"/>
      </id>
      <discriminator type="org.hibernate.type.StringType">
         <column name="Hjtype"/>
      </discriminator>
      <property access="org.jvnet.hyperjaxb2.runtime.hibernate.accessor.CheckingPropertyAccessor"
                name="Name">
         <column name="Name_"/>
         <type name="org.hibernate.type.StringType"/>
      </property>
      <property access="org.jvnet.hyperjaxb2.runtime.hibernate.accessor.CheckingPropertyAccessor"
                name="Email">
         <column name="Email"/>
         <type name="org.hibernate.type.StringType"/>
      </property>
      <property access="org.jvnet.hyperjaxb2.runtime.hibernate.accessor.CheckingPropertyAccessor"
                name="Comment">
         <column name="Comment_"/>
         <type name="org.hibernate.type.StringType"/>
      </property>
      <property access="org.jvnet.hyperjaxb2.runtime.hibernate.accessor.CheckingPropertyAccessor"
                name="Institution">
         <column name="Institution"/>
         <type name="org.hibernate.type.StringType"/>
      </property>
      <subclass discriminator-value="org.morphml.metadata.schema.Person"
                name="org.morphml.metadata.schema.impl.PersonImpl"/>
   </class>
</hibernate-mapping>