<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping
  PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping auto-import="false" default-cascade="all-delete-orphan" default-lazy="false">
   <class discriminator-value="[org.morphml.morphml.schema.Feature]"
          name="org.morphml.morphml.schema.Feature"
          table="Feature">
      <id access="org.jvnet.hyperjaxb2.runtime.hibernate.accessor.CheckingPropertyAccessor"
          name="Hjid"
          type="org.hibernate.type.LongType">
         <generator class="native"/>
      </id>
      <discriminator type="org.hibernate.type.StringType">
         <column name="Hjtype"/>
      </discriminator>
      <property access="org.jvnet.hyperjaxb2.runtime.hibernate.accessor.CheckingPropertyAccessor"
                name="Notes">
         <column name="Notes"/>
         <type name="org.hibernate.type.StringType"/>
      </property>
      <property access="org.jvnet.hyperjaxb2.runtime.hibernate.accessor.CheckingPropertyAccessor"
                name="Name">
         <column name="Name_"/>
         <type name="org.hibernate.type.StringType"/>
      </property>
      <list access="org.jvnet.hyperjaxb2.runtime.hibernate.accessor.ListAccessor"
            name="Polygon"
            table="Feature_Polygon">
         <key>
            <column name="Feature_Polygon_Hjid"/>
         </key>
         <list-index>
            <column name="Feature_Polygon_Hjindex"/>
         </list-index>
         <one-to-many class="org.morphml.metadata.schema.Polygon"/>
      </list>
      <many-to-one class="org.morphml.metadata.schema.Annotation" name="Annotation">
         <column name="Annotation"/>
      </many-to-one>
      <list access="org.jvnet.hyperjaxb2.runtime.hibernate.accessor.ListAccessor"
            name="FreePoints"
            table="Feature_FreePoints">
         <key>
            <column name="Feature_FreePoints_Hjid"/>
         </key>
         <list-index>
            <column name="Feature_FreePoints_Hjindex"/>
         </list-index>
         <one-to-many class="org.morphml.morphml.schema.FreePoints"/>
      </list>
      <list access="org.jvnet.hyperjaxb2.runtime.hibernate.accessor.ListAccessor"
            name="Polyhedron"
            table="Feature_Polyhedron">
         <key>
            <column name="Feature_Polyhedron_Hjid"/>
         </key>
         <list-index>
            <column name="Feature_Polyhedron_Hjindex"/>
         </list-index>
         <one-to-many class="org.morphml.metadata.schema.Polyhedron"/>
      </list>
      <list access="org.jvnet.hyperjaxb2.runtime.hibernate.accessor.ListAccessor"
            name="Sphere"
            table="Feature_Sphere">
         <key>
            <column name="Feature_Sphere_Hjid"/>
         </key>
         <list-index>
            <column name="Feature_Sphere_Hjindex"/>
         </list-index>
         <one-to-many class="org.morphml.metadata.schema.Sphere"/>
      </list>
      <list access="org.jvnet.hyperjaxb2.runtime.hibernate.accessor.ListAccessor"
            name="Manifold"
            table="Feature_Manifold">
         <key>
            <column name="Feature_Manifold_Hjid"/>
         </key>
         <list-index>
            <column name="Feature_Manifold_Hjindex"/>
         </list-index>
         <one-to-many class="org.morphml.metadata.schema.Manifold"/>
      </list>
      <list access="org.jvnet.hyperjaxb2.runtime.hibernate.accessor.ListAccessor"
            name="Group"
            table="Feature_Group_">
         <key>
            <column name="Feature_Group__Hjid"/>
         </key>
         <list-index>
            <column name="Feature_Group__Hjindex"/>
         </list-index>
         <element type="org.hibernate.type.StringType">
            <column name="Hjvalue"/>
         </element>
      </list>
      <list access="org.jvnet.hyperjaxb2.runtime.hibernate.accessor.ListAccessor"
            name="Path"
            table="Feature_Path_">
         <key>
            <column name="Feature_Path__Hjid"/>
         </key>
         <list-index>
            <column name="Feature_Path__Hjindex"/>
         </list-index>
         <one-to-many class="org.morphml.morphml.schema.Path"/>
      </list>
      <many-to-one class="org.morphml.metadata.schema.Properties" name="Properties">
         <column name="Properties"/>
      </many-to-one>
      <subclass discriminator-value="org.morphml.morphml.schema.Feature"
                name="org.morphml.morphml.schema.impl.FeatureImpl"/>
   </class>
</hibernate-mapping>