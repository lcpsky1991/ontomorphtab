<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping
  PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping auto-import="false" default-cascade="all-delete-orphan" default-lazy="false">
   <class discriminator-value="[org.morphml.morphml.schema.Morphology]"
          name="org.morphml.morphml.schema.Morphology"
          table="Morphology">
      <id access="org.jvnet.hyperjaxb2.runtime.hibernate.accessor.CheckingPropertyAccessor"
          name="Hjid"
          type="org.hibernate.type.LongType">
         <generator class="native"/>
      </id>
      <discriminator type="org.hibernate.type.StringType">
         <column name="Hjtype"/>
      </discriminator>
      <property access="org.jvnet.hyperjaxb2.runtime.hibernate.accessor.CheckingPropertyAccessor"
                name="Notes">
         <column name="Notes"/>
         <type name="org.hibernate.type.StringType"/>
      </property>
      <property access="org.jvnet.hyperjaxb2.runtime.hibernate.accessor.CheckingPropertyAccessor"
                name="Name">
         <column name="Name_"/>
         <type name="org.hibernate.type.StringType"/>
      </property>
      <many-to-one class="org.morphml.morphml.schema.Morphology$GroupDetailsType"
                   name="GroupDetails">
         <column name="GroupDetails"/>
      </many-to-one>
      <many-to-one class="org.morphml.morphml.schema.Morphology$PropertyDetailsType"
                   name="PropertyDetails">
         <column name="PropertyDetails"/>
      </many-to-one>
      <property access="org.jvnet.hyperjaxb2.runtime.hibernate.accessor.CheckingPropertyAccessor"
                name="LengthUnits">
         <column name="LengthUnits"/>
         <type name="org.hibernate.type.StringType"/>
      </property>
      <many-to-one class="org.morphml.morphml.schema.Cells" name="Cells">
         <column name="Cells"/>
      </many-to-one>
      <many-to-one class="org.morphml.metadata.schema.Annotation" name="Annotation">
         <column name="Annotation"/>
      </many-to-one>
      <property access="org.jvnet.hyperjaxb2.runtime.hibernate.accessor.CheckingPropertyAccessor"
                name="VolumeUnits">
         <column name="VolumeUnits"/>
         <type name="org.hibernate.type.StringType"/>
      </property>
      <list access="org.jvnet.hyperjaxb2.runtime.hibernate.accessor.ListAccessor"
            name="Group"
            table="Morphology_Group_">
         <key>
            <column name="Morphology_Group__Hjid"/>
         </key>
         <list-index>
            <column name="Morphology_Group__Hjindex"/>
         </list-index>
         <element type="org.hibernate.type.StringType">
            <column name="Hjvalue"/>
         </element>
      </list>
      <many-to-one class="org.morphml.morphml.schema.Morphology$FeaturesType" name="Features">
         <column name="Features"/>
      </many-to-one>
      <many-to-one class="org.morphml.metadata.schema.Properties" name="Properties">
         <column name="Properties"/>
      </many-to-one>
      <subclass discriminator-value="org.morphml.morphml.schema.Morphology"
                name="org.morphml.morphml.schema.impl.MorphologyImpl"/>
   </class>
</hibernate-mapping>